SRCS   = ./src/main
TARGET = enum_induced_subtree
CC     = g++ 
SRCDIR = ./src

CXXFLAGS += -g -Wextra -O3 -std=c++11 -I./include 
LDFLAGS += -L./src -lboost_program_options
LIBS     += $(SRCDIR)/libeis
.DEFAULT: $(CXXProgram $(TARGET), $(SRCS))

.PHONY: clean
clean:
	rm $(TARGET) $(SRCDIR)/*.o $(SRCDIR)/*.a

.SUBDIRS: src
